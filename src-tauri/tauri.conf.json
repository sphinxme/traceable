{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "traceable",
  "version": "0.1.0",
  "identifier": "com.sphinx.traceable",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "deno task dev",
    "beforeBuildCommand": "deno task build"
  },
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": ["traceable"]
      }
    }
  },
  "app": {
    "windows": [
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
